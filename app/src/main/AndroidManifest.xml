<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.icommunity" >
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.DarkActionBar" >
        <activity
            android:name="android.icommunity.community.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="android.icommunity.community.HomeActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.ForumMainActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.ForumDetailActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.NoticeMainActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.NoticeDetailActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.NeedRepairMainActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.NeedRepairDetailActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.NeedRepairNewActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.SettingPersonalInfoDetail">
        </activity>
        <activity
            android:name="android.icommunity.community.LoginActivity">
        </activity>
        <activity
            android:name="android.icommunity.community.ComplainActivity">
        </activity>
    </application>

</manifest>
